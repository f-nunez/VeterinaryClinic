{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=SchedulingEmailSender;MultipleActiveResultSets=True;Trusted_Connection=true;TrustServerCertificate=True"
  },
  "EmailTemplateSetting": {
    "UrlQueryToConfirmAppointment": "https://localhost:7138/appointment/confirm?id={0}&language={1}"
  },
  "RabbitMqSetting": {
    "HostAddress": "amqp://guest:guest@127.0.0.1:5672/"
  },
  "SymmetricEncryptionSetting": {
    "HashAlgorithmName": "SHA256",
    "Iterations": 100,
    "OutputLength": 16,
    "Password": "Password",
    "Salt": "Salt"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}