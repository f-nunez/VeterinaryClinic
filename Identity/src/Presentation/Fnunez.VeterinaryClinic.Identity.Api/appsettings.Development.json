{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "IdentityConnection": "Server=localhost;Database=Identity;MultipleActiveResultSets=True;Trusted_Connection=true;TrustServerCertificate=True",
    "IdentityServerConnection": "Server=localhost;Database=IdentityServer;MultipleActiveResultSets=True;Trusted_Connection=true;TrustServerCertificate=True"
  },
  "CookiePolicySetting": {
    "MinimumSameSitePolicy": "Lax"
  },
  "IdentityServerSetting": {
    "EmitStaticAudienceClaim": true,
    "IssuerUri": "https://localhost:7066",
    "RaiseErrorEvents": true,
    "RaiseFailureEvents": true,
    "RaiseInformationEvents": true,
    "RaiseSuccessEvents": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}